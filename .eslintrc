{
  "env": {
    "es2021": true,
    "node": true
  },
  "extends": "airbnb-base",
  "overrides": [],
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module"
  },
  "rules": {
    "no-console": "off",
    "no-plusplus": "off",
    "import/extensions": "off",
    "import/no-unresolved": "off"
  }
}
